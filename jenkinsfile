pipeline {
    agent any
    stages {
        stage ('vcs') {
            steps {
                git url: 'https://github.com/ajayreddy123456/game-of-life.git',
                branch: 'game'
            }
        }
        stage ('package') {
            tools {
                jdk "JDK_8"
            }
            steps {
                sh 'mvn package'
            }
            }
        }
    }


